<%= form_for @lead do |f| %>
  <div class="col s2">
    <%= f.label :prefix %>
    <%= f.text_field :prefix %>
  </div>


  <div class="col s5">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>

  <div class="col s5">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>

  <div class="col s6">
    <%= f.label :organization_name %>
    <%= f.text_field :organization_name %>
  </div>

  <div class="col s6">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="col s4">
    <%= f.label :phone_number %>
    <%= f.telephone_field :phone_number %>
  </div>

  <div class="col s4">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div class="col s4">
    <%= f.label :website %>
    <%= f.url_field :website %>
  </div>



  <%= f.submit "Next", class: 'btn'%>
<% end %>
